<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <context-param>
      <param-name>configuration</param-name>
      <param-value>development</param-value>
    </context-param>
    
    <filter>
        <filter-name>SimpleApplication</filter-name>
        <filter-class>wicket.protocol.http.WicketFilter</filter-class>
        <init-param>
          <param-name>applicationClassName</param-name>
          <param-value>org.senro.examples.simple.SimpleExampleApplication</param-value>
        </init-param>
        <init-param>
          <param-name>filterPath</param-name>
          <param-value></param-value>
        </init-param>
    </filter>

    <filter-mapping>
        <filter-name>SimpleApplication</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>