<?xml version="1.0"?>
<ComponentTemplate xmlns="http://www.siveco.ro/senro/ComponentTemplate" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.siveco.ro/senro/ComponentTemplate ComponentTemplate.xsd">
	<Layout>
		<Grid id="grid1" name="grid1">
			<Columns>
				<Column>
					<Alignment>Left</Alignment>
				</Column>
				<Column>
					<Alignment>Center</Alignment>
				</Column>
			</Columns>
			<Rows>
				<Row>
					<Alignment>Top</Alignment>
				</Row>
			</Rows>
			<Components>
				<Tree id="tree1" name="menu" row="0" col="0">
					<Iterator id="iter1" name="menuIterator" list="@{metadata.allEntities.values()}">
						<TreeNode>
							<Text>@{menuIterator.currentItem.qualifiedName}</Text>
						</TreeNode>
					</Iterator>
				</Tree>
				<RootPanel id="rootPanel" name="rootPanel" row="0" col="1" />
			</Components>
		</Grid>
	</Layout>

	<Server/>
	<Client/>

	<Params>
	</Params>

	<Associations/>
</ComponentTemplate>