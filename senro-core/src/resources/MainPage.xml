<?xml version="1.0"?>
<ComponentTemplate xmlns="http://www.siveco.ro/senro/DesignerComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.siveco.ro/senro/DesignerComponent ComponentTemplate.xsd">
	<Layout>
		<Grid id="grid1" name="grid1">
			<Columns>
				<Column>
					<Alignment>Left</Alignment>
				</Column>
				<Column>
					<Alignment>Center</Alignment>
				</Column>
			</Columns>
			<Rows>
				<Row>
					<Alignment>Center</Alignment>
				</Row>
				<Row>
					<Alignment>Center</Alignment>
				</Row>
			</Rows>
			<Components>
				<Tree id="tree1" name="menu" row="1" col="1">
				    <TreeNode>
						<Label id="lbl123" name="lbl123">
							<Text>1234</Text>
						</Label>
					</TreeNode>
					<Iterator id="iter1" name="menuIterator" list="@{allEntities}">
						<TreeNode>
							<Label>
								<Text>@{menuIterator.currentItem.metadata.name}</Text>
							</Label>
						</TreeNode>
					</Iterator>
				</Tree>
				<RootPanel id="rootPanel" name="rootPanel" row="1" col="2" />
				<Label id="label1" name="label1" row="2" col="1">
					<Text>Hello world</Text>
				</Label>
				<Label id="label2" name="label2" row="2" col="1">
					<Text>Mesajul de la label1 este: @{label1.text}</Text>
				</Label>
			</Components>
		</Grid>
	</Layout>

	<Server/>
	<Client/>

	<Params>
		<Parameter name="allEntities" type="java.util.List"/>
	</Params>

	<Associations/>
</ComponentTemplate>